<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon_splash"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_splash"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestFireBaseDB"
        tools:targetApi="31">
        <activity
            android:name=".UserManage.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserManage.SignInActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".UserManage.SignUpActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".activity.DayActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".activity.ExerciseActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".activity.DishActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".activity.ProfileActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".activity.DishesEditorActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.StatisticActivity"
            android:exported="true">
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>